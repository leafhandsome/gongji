  function renddata (arr1) {
     let vm = this;
     let result = [];
     let temp   = [];
     for (var i = 0; i < arr1.length; i++) {
       if (i > 0 && arr1[i] != arr1[i - 1]|| temp.length > 5) {
         result.push(temp);
         temp = [];
       }
       temp.push(arr1[i]);
       if (i + 1 == arr1.length) {
         result.push(temp);
       }
     }
     vm.rend = result;
   }
  </script>
